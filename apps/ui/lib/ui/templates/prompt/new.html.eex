<h1>New Prompt</h1>

<%= form_for @changeset, Routes.prompt_path(@conn, :create), [as: :prompt], fn f -> %>
  <%= text_input f, :type, placeholder: "Type" %>
  <%= text_input f, :source, placeholder: "Source" %>
  <%= text_input f, :url, placeholder: "URL" %>

  <%= submit "Create Prompt" %>
<% end %>
